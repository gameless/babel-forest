{
  "name": "babel-forest",
  "version": "1.0.0",
  "description": "A Book Jam game.",
  "repository": "https://github.com/samestep/babel-forest.git",
  "author": "Sam Estep",
  "license": "MIT",
  "main": "app/main.ts",
  "scripts": {
    "pages": "brunch build --production",
    "releases": "electron-forge make",
    "start": "electron-forge start",
    "test": "eslint app/*.ts"
  },
  "config": {
    "forge": {
      "make_targets": {
        "darwin": [
          "zip"
        ],
        "linux": [
          "zip"
        ],
        "win32": [
          "zip"
        ]
      }
    }
  },
  "devDependencies": {
    "@types/matter-js": "^0.10.2",
    "@types/seedrandom": "^2.4.28",
    "@types/underscore": "^1.8.18",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "brunch": "^2.10.17",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "4.0.0",
    "eslint": "^5.16.0",
    "typescript": "^3.4.5",
    "typescript-brunch": "^2.3.0"
  },
  "dependencies": {
    "electron-compile": "^6.4.4",
    "io-ts": "^1.8.6",
    "phaser": "^3.17.0",
    "seedrandom": "^3.0.1",
    "tone": "^13.4.9",
    "underscore": "^1.9.1"
  },
  "eslintConfig": {
    "extends": "plugin:@typescript-eslint/recommended",
    "rules": {
      "@typescript-eslint/explicit-function-return-type": {
        "allowExpressions": true
      },
      "@typescript-eslint/explicit-member-accessibility": "off",
      "@typescript-eslint/indent": [
        "error",
        2
      ],
      "@typescript-eslint/no-triple-slash-reference": "off"
    }
  }
}
